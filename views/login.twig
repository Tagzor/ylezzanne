	        <!-- Teeme mingi küpsiste pulli ka?
		<div class="checkbox">
	          <label>
	            <input type="checkbox" value="remember-me"> Remember me
	          </label>
	        </div>
		-->
	        <button class="btn btn-lg btn-primary btn-block" type="submit">Logi sisse</button>
		<button type="button" class="btn btn-info btn-block"><img src="id_pildid/id-kaart.png" alt="mobiilid">Sisene Mobiil-IDga</button>
		<button type="button" class="btn btn-info btn-block"><img src="id_pildid/mobiil-id.png" alt="mobiilid">Sisene Mobiil-IDga</button>
	</form>
</div>
{% endblock %}